<ul data-progress data-progress-current class="ly--progress">
  <li class="ly--progress-step ly--progress-step--complete">
    <svg width="24px" height="24px" viewBox="0 0 24 24">
      <circle cx="12" cy="12" r="12"></circle>
      <polygon points="10.3 13.6 7.7 11 6.3 12.4 10.3 16.4 17.8 9 16.4 7.6"></polygon>
    </svg>
    <p class="ly--progress-label">First step</p>
    <span class="ly--progress-line"></span>
  </li>
  <li class="ly--progress-step ly--progress-step--current">
    <svg>
      <circle cx="12" cy="12" r="12"></circle>
      <circle cx="12" cy="12" r="6"></circle>
    </svg>
    <p class="ly--progress-label">Second step</p>
    <span class="ly--progress-line"></span>
  </li>
  <li class="ly--progress-step ly--progress-step--incomplete">
    <svg>
      <circle cx="12" cy="12" r="12"></circle>
    </svg>
    <p class="ly--progress-label">Third step</p>
    <span class="ly--progress-line"></span>
  </li>
  <li class="ly--progress-step ly--progress-step--incomplete">
    <svg>
      <circle cx="12" cy="12" r="12"></circle>
    </svg>
    <p class="ly--progress-label">Fourth step</p>
    <span class="ly--progress-line"></span>
  </li>
</ul>
